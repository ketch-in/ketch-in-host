{
  "name": "ketch-in-host",
  "version": "0.3.3-beta.1",
  "productName": "Ketch-In",
  "description": "Electron-based App. The host's screen can be used as a sketchbook, allowing the host and attendees to draw.",
  "main": "src/main/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ketch-in/ketch-in-host"
  },
  "keywords": [
    "Electron",
    "Google Meet"
  ],
  "author": "Sotaneum",
  "license": "MIT",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "tsc && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts . --fix",
    "clean": "rm -rf ./src/**/*.js* || rm -rf ./out"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "KETCH IN HOST"
          }
        },
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "rtcmulticonnection": "^3.7.1",
    "socket.io-client": "^4.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "electron": "15.3.1",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "typescript": "^4.5.2"
  },
  "workspaces": [
    "packages/components"
  ]
}
